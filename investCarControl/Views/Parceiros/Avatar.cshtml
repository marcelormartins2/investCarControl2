@model IEnumerable<InvestCarControl.Models.Parceiro>

@{
    ViewData["Title"] = "Parceiros";
}

@section heads
{
    <link rel="stylesheet" href="~/css/style.css" type="text/css" />

    <style>
        html {
            position: fixed;
        }

        .container {
            margin: 0px;
            margin: 0px 0;
        }

        .imageBox {
            width: 160px;
            height: 160px;
        }


        .action {
            width: 600px;
            height: 30px;
            margin: 0px 0;
        }

        .cropped > img {
            margin-right: 10px;
        }

        #centro {
            width: 138px;
            height: 138px;
            position: absolute;
            top: 6%;
            left: 6%;
        }
    </style>
}


@await Html.PartialAsync("_parceirosInicio")


<base href="/img/avatars/">

<div class="col-md-4">
    <div class="container">
        <div class="imageBox">
            <div class="thumbBox"></div>
            <img src="circulo.png" alt="Avatar" id="centro">
            <div class="spinner" style="display: none;">Carregando...</div>
        </div>
        <div class="action" style="font-size: 0.5rem">
            <a>
                <i class="fa fa-plus-square-o fa-4x" id="btnZoomIn" title="zoom in">&nbsp</i>
                <i class="fa fa-minus-square-o fa-4x" id="btnZoomOut" title="zoom out">&nbsp</i>
                <i class="fa fa-check-square-o fa-4x" id="btnCrop" title="Salvar">&nbsp</i>
                <label for="file">
                    <i class="fa fa-refresh fa-4x" title="Trocar foto">&nbsp</i>
                </label>
                <input id="file" type="file" accept="image/*" style="display:none" />
            </a>
            <a asp-area="" asp-controller="Parceiros" asp-action="Index">
                <i class="fa fa-trash-o fa-4x" id="btnCancel" title="Cancelar"></i>
            </a>
        </div>
        <div class="cropped"> </div>
        <img id="avatarIMG" src="data:image/jpeg;base64," />
    </div>
</div>

@await Html.PartialAsync("_parceirosFim")

@section scripts
{

    

    <script type="text/javascript">
        $(window).load(function () {

            var options =
            {
                thumbBox: '.thumbBox',
                spinner: '.spinner',
                imgSrc: 'default2.png'
            }
            var cropper = $('.imageBox').cropbox(options);
            $('#file').on('change', function () {
                var reader = new FileReader();
                reader.onload = function (e) {
                    options.imgSrc = e.target.result;
                    cropper = $('.imageBox').cropbox(options);
                }
                reader.readAsDataURL(this.files[0]);
                this.files = [];
            })
            $('#btnCrop').on('click', function () {
                var img = cropper.getAvatar()
                
                $('.cropped').append('<img src="' + img + '">');
            
            var formdata = new FormData();
            formdata.append("base64image", img);
            $.ajax({ url: "@Url.Action("SaveImage","Parceiros")", type: "POST", data: formdata, processData: false, contentType: false });
            })

            $('#btnZoomIn').on('click', function () {
                cropper.zoomIn();
            })
            $('#btnZoomOut').on('click', function () {
                cropper.zoomOut();
            })
        });
    </script>
}
